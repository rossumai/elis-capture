{
  "expo": {
    "sdkVersion": "33.0.0",
    "privacy": "public",
    "name": "Elis Capture",
    "slug": "elis-capture",
    "description": "Simple app, that let's you send documents to Elis with ease.",
    "version": "0.2.1",
    "orientation": "portrait",
    "primaryColor": "#2F72FF",
    "icon": "./src/images/android/playstore-icon.png",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#1b1922"
    },
    "splash": {
      "image": "./src/images/android/playstore-icon.png",
      "backgroundColor": "#1b1922",
      "resizeMode": "contain"
    },
    "ios": {
      "bundleIdentifier": "ai.rossum.elis.capture",
      "buildNumber": "0.2.1",
      "icon": "./src/images/android/playstore-icon.png",
      "supportsTablet": false,
      "isTabletOnly": false
    },
    "android": {
      "package": "ai.rossum.elis.capture",
      "versionCode": 1,
      "icon": "./src/images/android/playstore-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./src/images/android/playstore-icon.png",
        "backgroundColor": "#1b1922"
      },
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
      ],
      "splash": {
        "ldpi": "./src/images/android/mipmap-ldpi/ic_launcher.png",
        "mdpi": "./src/images/android/mipmap-mdpi/ic_launcher.png",
        "hdpi": "./src/images/android/mipmap-hdpi/ic_launcher.png",
        "xhdpi": "./src/images/android/mipmap-xhdpi/ic_launcher.png",
        "xxhdpi": "./src/images/android/mipmap-xxhdpi/ic_launcher.png",
        "xxxhdpi": "./src/images/android/mipmap-xxxhdpi/ic_launcher.png",
        "backgroundColor": "#1b1922",
        "resizeMode": "contain"
      }
    }
  }
}
